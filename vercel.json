  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek_api_key",
